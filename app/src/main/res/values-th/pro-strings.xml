    <!-- Separator text between the 'Log In' and 'Create Account' buttons on the PsiCash account sign in screen. -->
    <string name="psicash_account_separator_text">หรือ</string>

    <!--Generic 'close' alert prompt title-->
    <string name="psicash_webview_close_alert_title">ปิด</string>

    <!--Generic 'error' alert prompt title-->
    <string name="psicash_webview_error_alert_title">ผิดพลาด</string>

    <!--Generic 'close' button label -->
    <string name="label_close">ปิด</string>

