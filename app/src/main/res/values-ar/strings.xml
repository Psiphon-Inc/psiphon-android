  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_upgrade_available_bp">إصدارات جديدة للتطبيق متاحة.</string>
  <!--Notifications permission rationale last sentence-->
  <string name="notifications_permission_rationale_disable_any_time">يمكنك تعطيل الإشعارات في إعدادات التطبيق في أي وقت.</string>
