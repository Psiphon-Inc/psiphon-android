  <string name="unsafe_traffic_alert_notification_title">Psiphon MalAware</string>
  <!--Content of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_message">Phát hiện tuyến đường giao thông không an toàn! Bấm vào để tìm hiểu thêm.</string>
  <!--Title of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_message">Psiphon phát hiện và đã chặn không cho truy cập vào trang mạng có thể chứa mã độc. Bấm vào đường dẫn sau đây để tìm hiểu thêm.\n\nMã độc tìm thấy:</string>
  <!--Title of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message">Psiphon tự động chặn không cho truy cập vào trang mạng có thể chứa mã độc. Bạn có muốn được thông báo khi phát hiện chuyện này?\n\nThiết đặt này có thể thay đổi sau trong phần Thêm Tùy Chọn.</string>
  <!--More info with a URL link in the contents of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message_more_info">Tìm hiểu thêm về MalAware nơi đây:</string>
  <!--"Upstream proxy error" notification title-->
  <string name="notification_title_upstream_proxy_error">Lỗi proxy luồng trên</string>
  <!--"Upstream proxy error" notification text-->
  <string name="notification_text_upstream_proxy_error">Xin mở Psiphon lên và kiểm lại thiết đặt proxy.</string>
  <!--"Upstream proxy error" alert dialog title-->
  <string name="upstream_proxy_error_alert_title">Lỗi Proxy</string>
  <!--"Upstream proxy error" alert dialog message-->
  <string name="upstream_proxy_error_alert_message">Bạn cấu hình Psiphon để dùng proxy luồng trên. Tuy nhiên, chúng tôi dường như không thể kết nối với máy chủ Psiphon qua proxy đó.\n\nXin sửa chữa lại các thiết đặt và thử lại.\n\nXem nhật ký để có thông tin kỹ thuật.</string>
  <!--Notification title for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_title">Psiphon bị hỏng</string>
  <!--Notification short message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg">Xin gửi phản hồi cho chúng tôi</string>
  <!--Notification longer message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg_long">Xin bấm vào thông báo này để gửi phản hồi cho chúng tôi. Việc này sẽ giúp chúng tôi hiểu vấn đề hơn để sửa chữa lẹ hơn.</string>
  <!--Data collection disclosure prompt button text-->
  <string name="vpn_data_collection_disclosure_accept_btn_text">Tôi chấp nhận</string>
  <!--Data collection disclosure prompt title-->
  <string name="vpn_data_collection_disclosure_prompt_title">Thông tin về thu thập dữ liệu</string>
  <!--First sentence of the data collection disclosure prompt body. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="vpn_data_collection_disclosure_top">%s dùng dịch vụ Android VPN để giúp bạn truy cập vào mạng không bị kiểm duyệt. Chúng tôi thu thập, tổng hợp, và lưu trữ các thông tin sau đây:</string>
  <!--Data collection disclosure list item-->
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp2">Email của bạn, khi bạn gửi chúng tôi dữ liệu chẩn đoán hoặc phản hồi sự cố hỏng. Cho biết email hay không trong phản hồi là tùy ý bạn.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp3">Tên người dùng, mật khẩu và có thể email của tài khoản PsiCash của bạn.</string>
