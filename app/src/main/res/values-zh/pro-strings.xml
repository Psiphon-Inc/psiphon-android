    <string name="custom_proxy_header_group_1">标题1</string>
    <string name="custom_proxy_header_group_2">标题2</string>
    <string name="custom_proxy_header_group_3">标题3</string>
    <string name="custom_proxy_header_group_4">标题4</string>
