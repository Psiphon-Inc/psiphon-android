  <!--First sentence of the notifications permission rationale message. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="notifications_permission_rationale_intro">%s gebruikt meldingen om u belangrijke informatie te geven wanneer de app op de achtergrond draait. Dit omvat:</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_vpn_state_bp">Wijzigingen in uw VPN-verbindingsstatus.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_connection_problems_bp">Problemen met uw verbinding.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_crash_reports_bp">Crashrapporten.</string>
  <!--Notifications permission rationale list item, do not translate or transliterate "MalAware"-->
  <string name="notifications_permission_rationale_malaware_alerts_bp">MalAware waarschuwingen.</string>
  <!--Notifications permission rationale list item-->
  <string name="notifications_permission_rationale_upgrade_available_bp">Nieuwe app-versies beschikbaar.</string>
  <!--Notifications permission rationale last sentence-->
  <string name="notifications_permission_rationale_disable_any_time">U kunt meldingen op elk moment uitschakelen in de app-instellingen.</string>
