  <!--Title of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_title">Psiphon MalAware</string>
  <!--Content of the toolbar notification which is shown to the user when Psiphon server detects an unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_notification_message">Malwareverkeer gedetecteerd! Klik voor meer informatie.</string>
  <!--Title of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which is shown to the user when they click toolbar notification of unsafe Internet traffic-->
  <string name="unsafe_traffic_alert_dialog_message">Psiphon heeft toegang tot potentieel kwaadaardige sites op internet gedetecteerd en geblokkeerd. Volg de onderstaande links voor meer informatie.\n\nGedetecteerde malware:</string>
  <!--Title of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_title">Psiphon MalAware</string>
  <!--Content of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message">Psiphon blokkeert automatisch toegang tot potentieel kwaadaardige sites op internet. Wilt u op de hoogte worden gesteld wanneer dit gebeurt?\n\nDeze instelling kan later gewijzigd worden in Meer Opties.</string>
  <!--More info with a URL link in the contents of the alert dialog which prompts the user to enable or disable unsafe Internet traffic alerts-->
  <string name="unsafe_traffic_alert_prompt_message_more_info">Lees hier meer over MalAware:</string>
  <!--"Upstream proxy error" notification title-->
  <string name="notification_title_upstream_proxy_error">Upstream proxy fout</string>
  <!--"Upstream proxy error" notification text-->
  <string name="notification_text_upstream_proxy_error">Open Psiphon en controleer proxy-instellingen.</string>
  <!--"Upstream proxy error" alert dialog title-->
  <string name="upstream_proxy_error_alert_title">Proxy Fout</string>
  <!--"Upstream proxy error" alert dialog message-->
  <string name="upstream_proxy_error_alert_message">U heeft Psiphon geconfigureerd om een upstream proxy te gebruiken. Het lijkt er echter op dat we geen verbinding kunnen maken met een Psiphon-server via die proxy.\n\nCorrigeer de instellingen en probeer opnieuw.\n\nZie logs voor technische informatie.</string>
  <!--Notification title for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_title">Psiphon is gecrasht</string>
  <!--Notification short message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg">Stuur ons feedback.</string>
  <!--Notification longer message for the 'Psiphon crash detected' notification-->
  <string name="psiphon_native_crash_notification_msg_long">Klik op deze melding om ons feedback te sturen. Dit helpt ons het probleem te begrijpen zodat we het sneller kunnen oplossen.</string>
  <!--Data collection disclosure prompt button text-->
  <string name="vpn_data_collection_disclosure_accept_btn_text">Ik accepteer</string>
  <!--Data collection disclosure prompt title-->
  <string name="vpn_data_collection_disclosure_prompt_title">Bekendmaking gegevensverzameling</string>
  <!--First sentence of the data collection disclosure prompt body. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
  <string name="vpn_data_collection_disclosure_top">%s gebruikt de Android VPN-service om u ongecensureerde toegang tot internetinhoud te bieden. We verzamelen, bundelen en slaan de volgende informatie op:</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp1">Uw stad, land, ISP en optioneel uw geschatte locatie. De stad, het land en de ISP worden afgeleid van het IP-adres waarmee u verbinding maakt met onze servers, en het IP-adres zelf wordt nooit gelogd. Deze gegevens worden verzameld voor gebruiks- en prestatiestatistieken en servicegezondheid.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp2">Uw e-mail, wanneer u ons een diagnose- of crash-logfeedback stuurt. Het opgeven van e-mail in de feedback is optioneel.</string>
  <!--Data collection disclosure list item-->
  <string name="vpn_data_collection_disclosure_bp3">Uw PsiCash account gebruikersnaam, wachtwoord en optioneel e-mail.</string>
  <!--Notifications permission rationale alert title. The format qualifier %s will be replaced by the app name, either 'Psiphon' or 'Psiphon Pro' and should be placed according to the rules of the translation language.-->
