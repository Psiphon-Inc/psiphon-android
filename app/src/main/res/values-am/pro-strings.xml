    <string name="rate_unit_mbps_bits">Mbps</string>
    <string name="rate_unit_gbps_bits">Gbps</string>
    <string name="rate_unit_tbps_bits">Tbps</string>
    <string name="rate_unit_pbps_bits">Pbps</string>


