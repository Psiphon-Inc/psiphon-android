    <string name="btn_disconnect">Αποσύνδεση</string>

    <!--Do not translate or transliterate 'Psiphon'. An alert dialog with this title is shown when the user connects from a region where free service is no longer offered, asking the user to choose an option to continue using the app.-->
    <string name="purchase_required_prompt_title">Πώς να αποκτήσετε πρόσβαση στο Psiphon στην περιοχή σας.</string>

    <!-- Label on a button. Clicking this button will buy 1 hour of Speed Boost. Translate number as well. -->
    <string name="speed_boost_product_1_hr">1 ώρα</string>

    <!-- Label on a button. Clicking this button will buy 1 day (24hr) of Speed Boost. Translate number as well. -->
    <string name="speed_boost_product_1_day">1 μέρα</string>

    <!-- Label on a button. Clicking this button will buy 1 week (7days) of Speed Boost. Translate number as well. -->
    <string name="speed_boost_product_1_week">1 εβδομάδα</string>

    <!-- Label on a button. Clicking this button will buy 1 months (31days) of Speed Boost. Translate number as well. -->
    <string name="speed_boost_product_1_month">1 μήνας</string>

    <!-- This format string specifies the order of 'days' and 'hours' in the date interval string where the first argument is 'days' and the seconds is 'hours'. Examples: "20 d 11 hr", "1 d 2 hr", etc.
     Reorder the format specifiers '%1$s' and '%2$s' if in the translated language ordering 'days' and 'hours' in date interval string is different from English. -->
    <string name="speed_boost_left_day_hour_ordered">%1$s%2$s</string>

    <!-- Do not translate or transliterate 'PsiCash'. PsiCash is a unit of currency here. Reorder the format specifier '%s' if in the translated language ordering of unit and amount is different from English. -->
    <string name="psicash_price_format">%s PsiCash</string>

    <!-- Icon description for the Conduit app icon, do not translate or transliterate 'Conduit'. -->
