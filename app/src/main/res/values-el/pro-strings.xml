    <string name="launch_conduit_action">Εκκίνηση Conduit</string>

    <!-- Clickable text label to open the Play Store page for the Psiphon Pro app, do not translate or transliterate 'Psiphon Pro'. -->
    <string name="update_psiphon_pro_title">Ενημέρωση Psiphon Pro</string>

